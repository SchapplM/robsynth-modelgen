       if `mod`(i, 10) = 3 then XZ[trunc((1/10)*i)+1, 1] := 0; printf(&quot;XZ%d \134n&quot;, trunc((1/10)*i)+1): end if:
       if `mod`(i, 10) = 4 then YY[trunc((1/10)*i)+1, 1] := 0; printf(&quot;YY%d \134n&quot;, trunc((1/10)*i)+1): end if:
       if `mod`(i, 10) = 5 then YZ[trunc((1/10)*i)+1, 1] := 0; printf(&quot;YZ%d \134n&quot;, trunc((1/10)*i)+1): end if:
       if `mod`(i, 10) = 6 then ZZ[trunc((1/10)*i)+1, 1] := 0; printf(&quot;ZZ%d \134n&quot;, trunc((1/10)*i)+1): end if:
       if `mod`(i, 10) = 7 then mX[trunc((1/10)*i)+1, 1] := 0; printf(&quot;MX%d \134n&quot;, trunc((1/10)*i)+1): end if:
       if `mod`(i, 10) = 8 then mY[trunc((1/10)*i)+1, 1] := 0; printf(&quot;MY%d \134n&quot;, trunc((1/10)*i)+1): end if:
       if `mod`(i, 10) = 9 then mZ[trunc((1/10)*i)+1, 1] := 0; printf(&quot;MZ%d \134n&quot;, trunc((1/10)*i)+1): end if:

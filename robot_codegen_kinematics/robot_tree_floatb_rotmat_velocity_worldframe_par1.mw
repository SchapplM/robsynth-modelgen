</Group><CodeEditor-ExecGroup view="presentation" labelreference="L1736" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">rD_W_Si := Matrix(3, NL):
for i to NL do
  # [Ortmaier2014] (7.17) (S.118), [KhalilDombre2002] (9.15)
  rD_W_Si(1 .. 3, i) := Matrix(rD_W_i(1 .. 3, i)) + Matrix(CrossProduct(omega_W_i(1 .. 3, i), r_W_i_Si(1 .. 3, i))):
  printf(&quot;%s. Geschwindigkeit f\303\274r K\303\266rperschwerpunkt %d aufgestellt.\134n&quot;, FormatTime(&quot;%Y-%m-%d %H:%M:%S&quot;), i-1):#0=Basis
  if use_simplify=1 then
    tmp_t1:=time():
    tmp_l1 := length(rD_W_Si(1 .. 3, i)):
    rD_W_Si(1 .. 3, i) := simplify2(rD_W_Si(1 .. 3, i)):
    tmp_l2 := length(rD_W_Si(1 .. 3, i)):
    tmp_t2:=time():
    printf(&quot;%s: Terme f\303\274r Schwerpunkts-Geschwindigkeiten vereinfacht. L\303\244nge: %d-&gt;%d. Rechenzeit %1.1fs.\134n&quot;, \134
      FormatTime(&quot;%Y-%m-%d %H:%M:%S&quot;), tmp_l1, tmp_l2, tmp_t2-tmp_t1):
  end if:
end do:

<Group labelreference="L1705" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for ii from 1 to NJ do
  for jj from 1 to RowDimension(kintmp_qt) do
    theta_qt(ii, 1) := subs( { kintmp_t(jj, 1) = kintmp_qt(jj, 1) }, theta_qt(ii, 1) ): 
  end do:
end do:</Text-field>
</Input>

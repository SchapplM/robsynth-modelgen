</Group><CodeEditor-ExecGroup view="presentation" labelreference="L3221" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion2" expanded="true" visible="true" pixel-width="697" pixel-height="145" code-language="text/maple" autoexpand="false" wrapping="false" show-border="true" code-line-numbers="true">for i to 3 do
	if JB1(1,i) = 0 and JB1(2,i) = 0 and JB1(3,i) = 0
	and JB1(i,1) = 0 and JB1(i,2) = 0 and JB1(i,3) = 0 then
		JB1(i,i) := 1:	end if:
end do:</EC-CodeEditor></CodeEditor-ExecGroup>
